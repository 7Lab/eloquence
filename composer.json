{
    "name": "siteonline/eloquence",
    "description": "Flexible Searchable, Mappable, Metable, Validation and more extensions for Laravel Eloquent ORM.",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jarektkaczyk/eloquence/issues",
        "source": "https://github.com/jarektkaczyk/eloquence"
    },
    "keywords": [
        "laravel",
        "eloquent",
        "metable",
        "searchable",
        "mappable",
        "mutable"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://git.falcon.zone/siteonline/eloquence-metable.git"
        },
        {
            "type": "vcs",
            "url": "https://git.falcon.zone/siteonline/eloquence-mutable.git"
        }
    ],
    "authors": [
        {
            "name": "Jarek Tkaczyk",
            "email": "jarek@softonsofa.com",
            "homepage": "http://softonsofa.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "sofa/eloquence-base": "v7.1",
        "sofa/eloquence-metable": "dev-lv7",
        "sofa/eloquence-mutable": "dev-lv7",
        "sofa/eloquence-mappable": "v7.0",
        "sofa/eloquence-validable": "v7.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Sofa\\Eloquence\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit"
    }
}
